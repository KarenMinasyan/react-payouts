{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app","prettier"],
  "parserOptions": {
    "ecmaFeatures": {"jsx": true},
    "ecmaVersion": "latest"
  },
  "plugins": ["react","import","prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {"printWidth":  100, "trailingComma":  "es5", "semi":  true, "jsxSingleQuote":  true, "singleQuote": true, "useTabs":  true, "arrowParens":  "avoid"}
    ]
  }
}
